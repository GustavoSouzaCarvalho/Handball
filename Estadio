public class Estadio {
	private Time time;
	private Integer id;
	private String nome;
	public Estadio(Time time, Integer id, String nome) {
		this.time = time;
		this.id = id;
		this.nome = nome;
	}
	public Time getTime() {
		return time;
	}
	public void setTime(Time time) {
		this.time = time;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	@Override
	public String toString() {
		return "\n\ttime: " + time + "\n\tid: " + id + "\n\tnome: " + nome;
	}
	
}
