package handball;

public class Estadio {
	private Integer id;
	private String nome;
	public Estadio() {
		
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	@Override
	public String toString() {
		return "\n\tid: " + id + "\n\tnome: " + nome;
	}
}
