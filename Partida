import java.time.LocalDate;	import java.util.List;	import java.util.ArrayList;
public class Partida {
	private List<Time> time = new ArrayList<>();
	private Endereco endereco;
	private Estadio estadio;
	private Integer id;
	private LocalDate data;
	private Integer pontmand;
	private Integer pontvisi;
	public Partida(Endereco endereco, Estadio estadio, Integer id, LocalDate data, Integer pontmand,
			Integer pontvisi) {
		this.endereco = endereco;
		this.estadio = estadio;
		this.id = id;
		this.data = data;
		this.pontmand = pontmand;
		this.pontvisi = pontvisi;
	}
	public void addTime(Time t) {
		time.add(t);
	}
	public void removeTime(Time t) {
		time.remove(t);
	}
	public Endereco getEndereco() {
		return endereco;
	}
	public void setEndereco(Endereco endereco) {
		this.endereco = endereco;
	}
	public Estadio getEstadio() {
		return estadio;
	}
	public void setEstadio(Estadio estadio) {
		this.estadio = estadio;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public LocalDate getData() {
		return data;
	}
	public void setData(LocalDate data) {
		this.data = data;
	}
	public Integer getPontmand() {
		return pontmand;
	}
	public void setPontmand(Integer pontmand) {
		this.pontmand = pontmand;
	}
	public Integer getPontvisi() {
		return pontvisi;
	}
	public void setPontvisi(Integer pontvisi) {
		this.pontvisi = pontvisi;
	}
	@Override
	public String toString() {
		return "\n\ttime: " + time + "\n\tendereco: " + endereco + "\n\testadio: " + estadio + "\n\tid: " + id + "\n\tdata: "
				+ data + "\n\tpontuacao mandante: " + pontmand + "\n\tpontuacao visitante: " + pontvisi;
	}

}
